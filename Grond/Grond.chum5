    <contact>
      <name>Donovan Lombardi</name>
      <role>Fixer</role>
      <location>Puyallup City</location>
      <connection>6</connection>
      <loyalty>1</loyalty>
      <metatype>Ork</metatype>
      <gender>Male</gender>
